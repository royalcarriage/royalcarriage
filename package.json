{
  "name": "royalcarriage",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "audit:images:content": "node ./scripts/audit-images-content.mjs",
    "install:all": "pnpm install",
    "build:admin": "pnpm --filter admin build",
    "build:airport": "pnpm --filter airport-service build",
    "build:corporate": "pnpm --filter corporate-service build",
    "build:wedding": "pnpm --filter wedding-service build",
    "build:partybus": "pnpm --filter partybus-service build",
    "build:functions": "pnpm --filter royalcarriage-functions build",
    "build:all-sites": "pnpm run build:airport && pnpm run build:corporate && pnpm run build:wedding && pnpm run build:partybus",
    "build": "pnpm run build:admin && pnpm run build:all-sites && pnpm run build:functions",
    "deploy:admin": "firebase deploy --only hosting:admin",
    "deploy:airport": "firebase deploy --only hosting:airport",
    "deploy:corporate": "firebase deploy --only hosting:corporate",
    "deploy:wedding": "firebase deploy --only hosting:wedding",
    "deploy:partybus": "firebase deploy --only hosting:partybus",
    "deploy:all-sites": "firebase deploy --only hosting:airport,hosting:corporate,hosting:wedding,hosting:partybus",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy": "firebase deploy",
    "lint": "pnpm -w -r run lint",
    "lint:fix": "pnpm -w -r run lint --filter ./ -- \"--fix\"",
    "start:emulators": "bash ./scripts/start-local-emulators.sh .env.local"
  },
